{
	"name": "bcassol.com",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev --port 4444 --open --host 0.0.0.0",
		"build": "svelte-kit build",
		"deploy": "rsync -arvz -e 'ssh -p 48124' --progress --delete build/* root@167.71.127.126:/var/www/html/bcassol.com",
		"prod": "npm run build && npm run deploy",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview --port 4444",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"docker-prune": "docker system prune --all --volumes",
		"docker-bash": "docker exec -it site_web_1  /bin/bash",
		"docker-bash2": "docker run -it site_web /bin/bash",
		"docker-host-npm-modules": "docker exec -it site_web_1 sh -c \"npm install\"",
		"docker-dev": "docker-compose up --build",
		"docker-npm-update": "docker exec -it site_web_1 bash -c \"npm update\"",
		"docker-prod": "docker run -it --rm -v ~/.ssh/id_rsa:/root/.ssh/id_rsa:ro site_web bash -c \"npm run prod\""
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.1",
		"svelte": "^3.44.3",
		"svelte-check": "^2.2.11",
		"svelte-preprocess": "^4.10.1",
		"tslib": "^2.3.1",
		"typescript": "^4.5.4"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-node": "^1.0.0-next.56",
		"@sveltejs/adapter-static": "^1.0.0-next.22"
	}
}